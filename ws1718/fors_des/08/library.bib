@misc{Rivero.,
 abstract = {Recent research has presented the question of the civilian control of military as an agency

relation in which the latter is able to extract rents from the former using the threat of a military

dictatorship. In this paper, I argue that this conceptualization fails to account for many important empirical regularities and propose a different framework in which the military is composed by different factions that counter-balance each other. I formalize this argument in a simple model that analyzes the conditions under which this heterogeneous military decides to intervene in politics jointly. The model also produces predictions about the occurrence of counter-coups and for the formation of different kinds of military regimes. Using a dataset covering 168 countries from 1950 to 2000, I show that military coups are associated with military spending only when the military is not in power. This result is supported by evidence regarding a distinction between civilian governments (democratic or not) and military governments. Finally, some preliminary results indicate that corporate interests are not usually pursued by those officers holding the institutional representation of the military, but by their subordinates.},
 author = {Rivero, Gonzalo},
 title = {Oligopoly of Violence: Civilian Control of a Heterogeneous Military},
 keywords = {civilian regime;military;military dictatorship}
}


