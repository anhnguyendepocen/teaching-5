@misc{Schneider.28.02.2010,
 abstract = {This paper presents a theory of international political budget cycles. I argue that governments attempt to increase their monetary transfers as well as other benefits from international organizations above average levels in the preelection period hoping to appear politically competent to their voters if they improve their country's position during international negotiations. The theory discusses the conditions under which international political budget cycles occur and derives several empirical implications for opportunistic bargaining in the European Union, a region in which international distributional negotiations are highly salient for individuals in 27 member states. A time-series cross sectional analysis of EU member states from 1977-2006 supports the existence of an international political budget cycle and points to the importance of accounting for such cycles when analyzing electoral politics of governments around the globe.},
 author = {Schneider, Christina},
 date = {28.02.2010},
 title = {Globalizing Electoral Politics:~International Political Budget Cycles in the European Union},
 urldate = {16.09.2011}
}


