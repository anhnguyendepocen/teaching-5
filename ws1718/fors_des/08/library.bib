@misc{Teorell.0205.09.2004,
 abstract = {The purpose of this paper is to address two short-comings in the present large-n literature on determinants of democratization: that of partial accounts and of regional confinement. We thus, first, assess the amount of variance in democratization that can be explained by the full range of global determinants suggested in previous studies. Second, we explore the extent to which apparent regional effects can be explained with reference to the global determinants. Using both Freedom House and Polity as democracy indicators, our tests are performed using time-series cross-section data on some 190 countries during the last 30 years. We find that even the most comprehensive models can only explain some 6 percent of the variation in short-term democratization. Moreover, the regional effects, entered as dummies for seven world regions, either disappear once global determinants are controlled for (Freedom House), or add miniscule explanatory power in terms of understanding the dynamics of regime change (Polity). We conclude by discussing alternative implications for future work in the field.},
 author = {Teorell, Jan and Hadenius, Axel},
 year = {02-05.09.2004},
 title = {Global and Regional Determinants of Democratization},
 keywords = {democratization;quantitative research},
 series = {Annual Meeting of the American Political Science Association}
}


