@article{Kohler.2008,
 abstract = {In recent years, electoral processes in non-democratic settings have been analysed either within the framework of transitology or disregarded entirely by scholars of comparative politics. Analysing the Egyptian case, this article proposes a different conceptual framework. The interaction between electoral institutions and authoritarian dynamics is conceptualized in terms of the relationship between formal and informal institutions. In the Egyptian authoritarian political system, informal mechanisms of neopatrimonial rule not only take precedence over formalized rules and procedures but integrate formal electoral institutions into the authoritarian system. Drawing on empirical evidence from legislative elections under the rule of President Husni Mubarak, this article identifies three main functions for electoral processes in nondemocratic settings: (1) Electoral contests serve to periodically renew channels of clientelist inclusion, drawing both voters and deputies into networks of patronage culminating at the top of the political system. (2) Formal inclusion of parts of the opposition into the electoral arena enhances the range of means available to the ruling elite in order to control these actors. (3) Pitted against each other in electoral contests, individual members of the ruling elite's lower echelons are effectively controlled and tied to the informal structures of rule. Thus, the principal traits of the Egyptian neopatrimonial regime remain unchanged, with formal electoral processes subverted by informal institutions of authoritarian rule to an extent as to fulfil distinctly authoritarian functions.},
 author = {K{\"o}hler, Kevin},
 year = {2008},
 title = {Authoritarian Elections in Egypt: Formal Institutions and Informal Mechanisms of Rule},
 keywords = {authoritarianism;Egypt;elections;informal institutions;Middle East},
 urldate = {27.09.2011},
 pages = {974--990},
 volume = {15},
 number = {5},
 issn = {1351-0347 print / 1743-890X online},
 journal = {Democratization}
}


@misc{Kohler.2010,
 abstract = {In this paper, I examine the rarely appreciated fact that Middle Eastern and North African legislatures display marked differences with respect to the degree of multipartyism in parliamentary composition. Despite the fact that the composition of all of these legislatures is the result of authoritarian elections, the average effective number of legislative parties (ENLP) varies from 1 to above 8 in different countries of the region. The argument takes the form of a `nested analysis' (Lieberman 2005): First, I fit multivariate OLS regression models to an original dataset of authoritarian elections and legislatures in the MENA encompassing 47 elections and 204 country year observations. I demonstrate that the inclusion of regime type variables distinguishing between civilian dictatorships on the one hand and military or monarchical regimes on the other significantly improves the explanatory power of the model even if alternative explanatory variables are controlled for. In the second part, I analyze five exemplary cases in more depth. The aim of this second step is to flesh out the explanatory hypotheses that emerged from the quantitative analysis in the first part and to hypothesize about the causal mechanisms involved. I argue that strategies of coalition management specific to the respective authoritarian subtype are major explanatory factors. By way of conclusion, I summarize my main findings and point to some unresolved issues in the argument.},
 author = {Koehler, Kevin},
 date = {2010},
 title = {Regimes and Institutions:~Patterns of Multipartyism in Authoritarian Parliaments in the Middle East and North Africa (MENA)},
 urldate = {27.09.2011}
}


