@misc{Kneip.,
 abstract = {Whether and in how far constitutional courts can contribute to successful processes of democratization or the establishment of the rule of law still remains unclear. While the impact of political actors on consolidation processes is apparent and has been widely analyzed, the distinctive role of judicial actors is not that obvious. The paper contributes to this incipient debate by discussing institutional prerequisites and actual court behaviour in young and consolidated democracies. After a short normative discussion on the role of constitutional courts in democracies (part 2), the paper identifies the institutional and factual prerequisites courts need for contributing to democratization. For this purpose, the different stages of democratization (liberalization, institutionalization, consolidation) are separated and the different effects of institutional rules and norms during these separate phases are discussed (parts 3 and 4 of the paper). The paper argues that institutional strength, institutional independence, and a democratic role orientation of courts are generally decisive factors for a positive role of (constitutional) courts in processes of democratization. It is further shown empirically that these prerequisites are distributed quite unevenly in today's world of young democracies.},
 author = {Kneip, Sascha},
 title = {The Role of Constitutional Courts in Democratization Processes: Theoretical Considerations and Empirical Trends: Paper prepared for the conference {\grqq}Advocates or Notaries of Democracy? A Comparative Socio-legal Analysis of the Role of Constitutional Courts}
}


