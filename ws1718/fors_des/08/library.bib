@misc{Gerschewski.02.05.09..2010,
 abstract = {The aim of the paper is twofold. On the one hand, it proposes a conjunctural theory for the stability (and change) of autocratic regimes that is based on three interdependent pillars: legitimation, repression and co-optation. It is argued that these three pillars provide an autocracy with vital functions to maintain stability over time. In the course of the paper, emphasis is put on concept-building. On the other hand, the paper tries to make sense of the heterogeneous picture East Asia presents. While there are as many autocracies that have initiated democratization processes as autocracies that have remained stable in this region, the paper tries to find explanations for this phenomenon by making methodically use of fuzzy set QCA. Against the backdrop of the three pillar theory, first preliminary empirical results will be shown. The paper identifies different stabilizing paths which suggest an empirically based, typological distinction of stable autocracies that follow either an over- or a de-politicizing path. It is argued that these autocratic regimes are not just different in degree, but different in kind.},
 author = {Gerschewski, Johannes},
 year = {2010},
 title = {Three Pillars of Stability:~Towards an Explanation of the Durability of Autocratic Regimes in East Asia},
 address = {Washington, D.C.},
 series = {Annual Meeting of the American Political Science Association, 02.-05.09.2010}
}


@article{Gerschewski.2007,
 abstract = {In October 2007 Japan extended its unilateral sanctions against North Korea. Owing to this decision, the focus shifted from recent positive signals in dealing with the denuclearization to last year's North Korean nuclear test and the following attempts to sanction the regime. Against this background, the analysis first provides a short overview of the latest research on sanctions and then analyzes the increasing external pressure on North Korea since Pyongyang's missile and nuclear tests in 2006. Sanctions had been imposed by the USA, Japan and, for the first time, in its history also by the UN. Whereas the Japanese sanctions failed to harm North Korea's elite due to its comprehensive approach, the more targeted and multilateral sanctions of the UN could have been more effective. Yet, these sanctions were undermined by the diverging national interests of South Korea and China. Their respective rationale aim at stabilizing North Korea rather than taking destabilising measures. From a theoretical point of view, sanctioning North Korea could make sense as targeted (financial) sanctions against the elite could drive a wedge between Pyongyang's hard- and soft-liners and thus weaken intraelite cohesiveness as well as reduce essential financial patronage resources.},
 author = {Gerschewski, Johannes},
 year = {2007},
 title = {Sanctions against North Korea: Effective Instrument or Mere Windowdressing},
 keywords = {Japan, North Korea, Sanctions},
 pages = {37--52},
 number = {6},
 journal = {Japan Aktuell}
}


