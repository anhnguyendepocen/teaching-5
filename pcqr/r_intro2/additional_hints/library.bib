@report{richterAussenwirtschaftsliberalisierungBeiAutokratien2011,
  location = {{Hamburg}},
  title = {Außenwirtschaftsliberalisierung Bei {{Autokratien}}: {{Determinanten}} Und {{Entwicklungspfade}}},
  abstract = {In einer Reihe von makro-quantitativen Studien wird eine seit den 1980er Jahren weltweit gestiegene außenwirtschaftliche Offenheit mit der dritten Demokratisierungswelle verknüpft. Insbesondere bei Entwicklungsländern, so wird argumentiert, hat ein zeitlich vorgelagerter politischer Liberalisierungsprozess den Abbau restriktiver außenwirtschaftlicher Regulierungen kausal verursacht bzw. unterstützt. Eine Untersuchung von Determinanten und Entwicklungspfaden innerhalb der Gruppe der Autokratien existiert dabei bisher nicht. Das Papier versucht diesem Defizite mit einem interregionalen Vergleich aller autoritären Regime zwischen 1978 und 2003 zu begegnen. Basierend auf statistischen Schätzungen mit Hilfe eines Datensatz, der neben bisher verwendeten Variablen auch den autoritären Regimetyp, die Ausstattung mit Ressourcen und die Existenz von Programmen des Internationalen Währungsfonds berücksichtig, lassen sich drei zentrale Schlussfolgerungen ziehen: Eine politische Liberalisierung spielt keine Rolle bei der Wahl des Außenhandelsregimes von Autokratien. Viel entscheidender ist die Ausstattung eines Landes mit Devisenreserven. Je höher diese sind, desto weniger neigt ein Land zum Abbau außenwirtschaftlicher Restriktionen. Zusätzlich lassen sich interessante und teilweise divergierende Trends bei der Ausgestaltung von außenwirtschaftlichen Restriktionen zwischen den verschiedenen autoritären Subtypen identifizieren. Eine zukünftige Forschung sollte sich vor allem auf eine bessere empirische Absicherung und eine theoretische Erklärung in Bezug auf diesen letzten Befund konzentrieren.},
  type = {UnpublishedWork},
  date = {2011-07-20T00:00:00},
  keywords = {PVS 2012},
  author = {Richter, Thomas},
  editora = {GIGA Institut für Nahost-Studien},
  editoratype = {collaborator}
}

