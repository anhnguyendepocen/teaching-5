@article{morlinoAreThereHybrid2009,
  title = {Are There Hybrid Regimes? {{Or}} Are They Just an Optical Illusion?},
  volume = {1},
  abstract = {In recent years there has been growing interest and a related literature on hybrid regimes. Is there a good definition of such an institutional arrangement? Are there actually sets of stabilized, political institutions that can be labelled in this way? Is it possible that within the widespread process of democracy diffusion these are only ‘transitional’ regimes and the most suitable distinction is still the old one, suggested by Linz and traditionally accepted, between democracy and authoritarianism? This article addresses and responds to these questions by pinpointing the pertinent analytic dimensions, starting with definitions of ‘regime’, ‘authoritarianism’, and ‘democracy’; by defining what a ‘hybrid 
regime’ is; by trying to answer the key question posed in the title; by disentangling the cases of proper hybrid regimes from the cases of transitional phases; and by proposing a typology of hybrid regimes. Some of the main findings and conclusions refer to the lack of institutions capable of performing their functions as well as the key elements for achieving possible changes towards democracy.},
  number = {2},
  journaltitle = {European Political Science Review},
  date = {2009-01-01T00:00:00},
  pages = {273-296},
  keywords = {regime; democracy; authoritarianism; hybrid regime; classification of regimes},
  author = {Morlino, Leonardo}
}

