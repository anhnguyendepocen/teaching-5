@article{bernholzConstitutionTotalitarianism1991,
  eprinttype = {jstor},
  eprint = {40751427},
  title = {The {{Constitution}} of {{Totalitarianism}}},
  volume = {147},
  issn = {09324569},
  abstract = {It is the purpose of the present paper to show that totalitarianism constitutes a rational system which binds population and leaders by stable long-lasting rules, which form an unwritten or written constitution. It is exactly this constitutional trait of totalitarianism which is one of the main differences distinguishing it from dictatorship or tyranny, since the latter are characterized by the uncontrolled discretionary power of the tyrant. Finally, necessary elements of the Constitution of Totalitarianism are identified. Es ist das Ziel des vorliegenden Beitrages zu zeigen, daß es sich beim Totalitarismus um ein rationales System handelt, das die Bevölkerung und die Führer durch stabile langfristig gültige Regeln bindet, die wiederum eine geschriebene oder ungeschriebene Verfassung repräsentieren. Es wird argumentiert, daß es gerade dieses Merkmal ist, welches totalitaristische Staatsformen in erster Linie von Diktaturen und Tyrannei unterscheidet, die durch die unkontrollierte Machtausiibung nach freiem Ermessen des Tyrannen charakterisiert sind. Schließlich werden notwendige Elemente der Verfassung des Totalitarismus herausgearbeitet.},
  number = {3},
  journaltitle = {Journal of Institutional and Theoretical Economics (JITE) / Zeitschrift für die gesamte Staatswissenschaft},
  date = {1991-01-01T00:00:00},
  pages = {425-440},
  author = {Bernholz, Peter}
}

