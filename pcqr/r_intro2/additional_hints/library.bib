@misc{basedauAufRuckzugKasernen2011,
  title = {Auf Dem {{Rückzug}} in Die {{Kasernen}}? {{Autoritäre Herrschaft}} Und Das {{Militär}} Im Subsaharischen {{Afrika}}: {{Paper}} Für Die {{Autorenkonferenz PVS}}-{{Sonderheft}} 2012 „{{Autokratien}} Im {{Vergleich}}“},
  abstract = {Trotz eines Rückgangs an offenen politischen Interventionen des Militärs im subsaharischen Afrika spricht vieles dafür, dass es einen Zusammenhang zwischen Merkmalen und Verhalten des Militärs und der Stabilität subsaharischer autokratischer Regime gibt. Die Forschung hat sich bislang aber wenig mit diesem Zusammenhang beschäftigt. Der vorliegende Beitrag überprüft anhand einer Reihe von eigens erhobenen Indikatoren, inwieweit es sich bei Afrikas autoritären Regimen um (zivilisierte) Militärregime handelt und inwieweit autokratische Regime von der Unterstützung der Streitkräfte abhängen. Ergebnisse auf Basis eines systematischen Vergleichs von verschiedenen Regimetypen sowie Evidenz aus Einzelfällen legen nahe, dass das Militär eine entscheidende Rolle bei der Stabilisierung autokratischer Regime spielt. Die zukünftige Forschung sollte sich mit weniger sichtbaren Einflussmöglichkeiten und der konditionierenden Wirkung von Kontextfaktoren beschäftigen.},
  date = {2011-01-01T00:00:00},
  keywords = {PVS 2012},
  author = {Basedau, Matthias and Elischer, Sebastian},
  editora = {German Institute of Global {and} Area Studies},
  editoratype = {collaborator}
}

